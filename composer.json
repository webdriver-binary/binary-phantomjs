{
    "name": "webdriver-binary/binary-phantomjs",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "A Composer package which installs the PhantomJS binary (Linux, Windows, Mac) into `bin` of your project",
    "require": {
        "php": ">=7.2",
        "composer-plugin-api": "^1.0 | ^2.0",
        "ext-openssl": "*"
    },
    "replace": {
        "vaimo/binary-phantomjs": "*",
        "lanfest/binary-phantomjs": "*"
    },
    "keywords": [
        "phantomjs",
        "headless",
        "binaries",
        "downloader",
        "phantom-installer"
    ],
    "extra": {
        "class": "WebdriverBinary\\PhantomInstaller\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "WebdriverBinary\\PhantomInstaller\\": "src"
        }
    }
}
